		{
			"name": "Patio - get",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code 200 - Lista de Pátios\", function () {\r",
							"    pm.expect(pm.response.code, \"Status code should be 200\").to.eql(200);\r",
							"});\r",
							"\r",
							"let body = null;\r",
							"try {\r",
							"    body = pm.response.json();\r",
							"} catch (e) {\r",
							"    body = null;\r",
							"}\r",
							"\r",
							"pm.test(\"JSON válido\", function () {\r",
							"    pm.expect(body, \"Resposta não é JSON\").to.be.an(\"object\").that.is.not.null;\r",
							"});\r",
							"\r",
							"let contentArray = null;\r",
							"let pagination = null;\r",
							"\r",
							"if (body && Array.isArray(body)) {\r",
							"    contentArray = body;\r",
							"} else if (body && typeof body === \"object\" && Array.isArray(body.content)) {\r",
							"    contentArray = body.content;\r",
							"    pagination = {\r",
							"        pageNumber: body?.pageable?.pageNumber,\r",
							"        pageSize: body?.pageable?.pageSize,\r",
							"        totalElements: body?.totalElements,\r",
							"        totalPages: body?.totalPages,\r",
							"        number: body?.number,\r",
							"        numberOfElements: body?.numberOfElements,\r",
							"        first: body?.first,\r",
							"        last: body?.last\r",
							"    };\r",
							"}\r",
							"\r",
							"pm.test(\"Response contém lista de pátios\", function () {\r",
							"    pm.expect(contentArray, \"Não foi possível localizar a lista de pátios (array raiz ou content[])\").to.be.an(\"array\");\r",
							"});\r",
							"\r",
							"if (contentArray) {\r",
							"    pm.test(\"Lista de pátios não está vazia\", function () {\r",
							"        pm.expect(contentArray.length, \"Nenhum pátio retornado\").to.be.greaterThan(0);\r",
							"    });\r",
							"\r",
							"    pm.test(\"Cada pátio possui campos esperados e tipos válidos\", function () {\r",
							"        contentArray.forEach((p, idx) => {\r",
							"            pm.expect(p, `Item ${idx} não é objeto`).to.be.an(\"object\");\r",
							"\r",
							"            pm.expect(p, `idPatio ausente em item ${idx}`).to.have.property(\"idPatio\");\r",
							"            pm.expect(\r",
							"                p.idPatio,\r",
							"                `idPatio inválido no item ${idx} (esperado number ou string não vazia)`\r",
							"            ).to.satisfy(v => typeof v === \"number\" || (typeof v === \"string\" && v.trim() !== \"\"));\r",
							"\r",
							"            pm.expect(p, `localizacao ausente em item ${idx}`).to.have.property(\"localizacao\");\r",
							"            pm.expect(\r",
							"                p.localizacao,\r",
							"                `localizacao inválido no item ${idx} (string não vazia)`\r",
							"            ).to.be.a(\"string\").and.to.satisfy(s => s.trim() !== \"\");\r",
							"\r",
							"            pm.expect(p, `quantidadeVagas ausente em item ${idx}`).to.have.property(\"quantidadeVagas\");\r",
							"            pm.expect(\r",
							"                p.quantidadeVagas,\r",
							"                `quantidadeVagas inválido no item ${idx} (deve ser número)`\r",
							"            ).to.satisfy(v => typeof v === \"number\" && !isNaN(v));\r",
							"        });\r",
							"    });\r",
							"}\r",
							"\r",
							"(function visualize() {\r",
							"    const rows = Array.isArray(contentArray) ? contentArray : [];\r",
							"    const mask = (s) => {\r",
							"        if (typeof s !== \"string\") return \"\";\r",
							"        if (s.length <= 2) return \"*\".repeat(s.length);\r",
							"        return s[0] + \"*\".repeat(Math.max(1, s.length - 2)) + s[s.length - 1];\r",
							"    };\r",
							"\r",
							"    const template = `\r",
							"    <style>\r",
							"      .pg { margin-top: 10px; font-family: sans-serif; }\r",
							"      table { border-collapse: collapse; width: 100%; font-family: sans-serif; }\r",
							"      th, td { border: 1px solid #ddd; padding: 8px; }\r",
							"      th { background: #f5f5f5; text-align: left; }\r",
							"      .empty { padding: 12px; color: #666; }\r",
							"    </style>\r",
							"    {{#if rows.length}}\r",
							"      <table>\r",
							"        <thead>\r",
							"          <tr>\r",
							"            <th>idPatio</th>\r",
							"            <th>localizacao</th>\r",
							"            <th>quantidadeVagas</th>\r",
							"          </tr>\r",
							"        </thead>\r",
							"        <tbody>\r",
							"          {{#each rows}}\r",
							"            <tr>\r",
							"              <td>{{this.idPatio}}</td>\r",
							"              <td>{{this.localizacao}}</td>\r",
							"              <td>{{this.quantidadeVagas}}</td>\r",
							"            </tr>\r",
							"          {{/each}}\r",
							"        </tbody>\r",
							"      </table>\r",
							"    {{else}}\r",
							"      <div class=\"empty\">Nenhum pátio encontrado.</div>\r",
							"    {{/if}}\r",
							"\r",
							"    {{#if pagination}}\r",
							"      <div class=\"pg\">\r",
