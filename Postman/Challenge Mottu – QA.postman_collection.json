		{
			"name": "Funcionario - get",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code 200 - Lista de Funcionários\", function () {",
							"  pm.expect(pm.response.code, \"Status code should be 200\").to.eql(200);",
							"});",
							"",
							"let body = null;",
							"try {",
							"  body = pm.response.json();",
							"} catch (e) {",
							"  body = null;",
							"  console.log(\"Falha ao fazer parse do JSON:\", e);",
							"}",
							"",
							"pm.test(\" JSON válido\", function () {",
							"  pm.expect(body, \"Resposta não é JSON\").to.be.an(\"object\").that.is.not.null;",
							"});",
							"",
							"let contentArray = null;",
							"let pagination = null;",
							"",
							"if (body && Array.isArray(body)) {",
							"  contentArray = body;",
							"} else if (body && typeof body === \"object\" && Array.isArray(body.content)) {",
							"  contentArray = body.content;",
							"  pagination = {",
							"    pageNumber: body?.pageable?.pageNumber,",
							"    pageSize: body?.pageable?.pageSize,",
							"    totalElements: body?.totalElements,",
							"    totalPages: body?.totalPages,",
							"    number: body?.number,",
							"    numberOfElements: body?.numberOfElements,",
							"    first: body?.first,",
							"    last: body?.last",
							"  };",
							"}",
							"",
							"pm.test(\"Response contém lista de funcionários\", function () {",
							"  pm.expect(contentArray, \"Não foi possível localizar a lista de funcionários (array raiz ou content[])\").to.be.an(\"array\");",
							"});",
							"",
							"if (contentArray) {",
							"  pm.test(\"Lista de funcionários não está vazia\", function () {",
							"    pm.expect(contentArray.length, \"Nenhum funcionário retornado\").to.be.greaterThan(0);",
							"  });",
							"",
							"  pm.test(\"Cada funcionário possui campos esperados e tipos válidos\", function () {",
							"    contentArray.forEach((f, idx) => {",
							"      console.log(`Funcionário [${idx}]:`, f);",
							"      ",
							"      pm.expect(f, `Item ${idx} não é objeto`).to.be.an(\"object\");",
							"      ",
							"      pm.expect(f, `idFuncionario ausente em item ${idx}`).to.have.property(\"idFuncionario\");",
							"      pm.expect(",
							"        f.idFuncionario,",
							"        `idFuncionario inválido no item ${idx} (esperado number ou string não vazia)`",
							"      ).to.satisfy(v => typeof v === \"number\" || (typeof v === \"string\" && v.trim() !== \"\"));",
							"",
							"      pm.expect(f, `nomeUsuario ausente em item ${idx}`).to.have.property(\"nomeUsuario\");",
							"      pm.expect(",
							"        f.nomeUsuario,",
							"        `nomeUsuario inválido no item ${idx} (string não vazia)`",
							"      ).to.be.a(\"string\").and.to.satisfy(s => s.trim() !== \"\");",
							"",
							"      pm.expect(f, `email ausente em item ${idx}`).to.have.property(\"email\");",
							"      pm.expect(",
							"        f.email,",
							"        `email inválido no item ${idx} (string não vazia)`",
							"      ).to.be.a(\"string\").and.to.satisfy(s => s.trim() !== \"\");",
							"",
							"      pm.expect(f, `senha ausente em item ${idx}`).to.have.property(\"senha\");",
							"      const senhaVal = f.senha;",
							"",
							"      if (senhaVal === null) {",
							"        console.log(`senha é null no item ${idx}`);",
							"      } else if (typeof senhaVal === \"string\") {",
							"        console.log(`senha é string no item ${idx}:`, senhaVal);",
							"      } else if (typeof senhaVal === \"object\") {",
							"        console.log(`senha é objeto no item ${idx}:`, senhaVal);",
							"      } else {",
							"        console.log(`senha tem tipo inesperado no item ${idx}:`, typeof senhaVal, senhaVal);",
							"      }",
							"      pm.expect([\"string\", \"object\"], `senha tipo inesperado no item ${idx}`).to.include(typeof senhaVal);",
							"      pm.expect([\"string\", \"object\", \"null\"], `senha tipo inesperado no item ${idx}`).to.include(senhaVal === null ? \"null\" : typeof senhaVal);",
							"    });",
							"  });",
							"}",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
