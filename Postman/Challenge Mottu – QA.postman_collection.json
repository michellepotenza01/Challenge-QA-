				{
					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"// Test: Status code should be 201 (Created)",
							"pm.test(\"Status code is 201 Created\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"// Test: Response should be JSON",
							"pm.test(\"Response is JSON\", function () {",
							"    pm.response.to.be.json;",
							"});",
							"",
							"// Test: Response should have required properties",
							"pm.test(\"Response has required properties\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData).to.have.property('idCliente');",
							"    pm.expect(jsonData).to.have.property('nome');",
							"    pm.expect(jsonData).to.have.property('email');",
							"    pm.expect(jsonData).to.have.property('cpf');",
							"});",
							"",
							"// Save clienteId to environment (outside of test block for reliability)",
							"if (pm.response.code === 201) {",
							"    try {",
							"        var jsonData = pm.response.json();",
							"        if (jsonData.idCliente) {",
							"            pm.environment.set(\"clienteId\", jsonData.idCliente);",
							"            console.log(\"✓ Cliente ID saved to environment: \" + jsonData.idCliente);",
							"            ",
							"            // Test to verify the variable was set correctly",
							"            pm.test(\"clienteId saved to environment\", function () {",
							"                pm.expect(pm.environment.get(\"clienteId\")).to.equal(jsonData.idCliente);",
							"            });",
							"        } else {",
							"            console.error(\"✗ Response does not contain idCliente property\");",
							"            pm.test(\"clienteId exists in response\", function () {",
							"                pm.expect(jsonData).to.have.property('idCliente');",
							"            });",
							"        }",
							"    } catch (e) {",
							"        console.error(\"✗ Error parsing response or setting clienteId: \" + e.message);",
							"        pm.test(\"Error setting clienteId\", function () {",
							"            pm.expect.fail(\"Failed to parse response or set clienteId: \" + e.message);",
							"        });",
							"    }",
							"} else {",
							"    console.warn(\"⚠ Status code is not 201, skipping clienteId save\");",
							"}"
						]
					}
				}
			],
